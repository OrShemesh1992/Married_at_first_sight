<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/profile"
    tools:context=".matchPage">


    studio and fire base

    <TextView
        android:id="@+id/name"
        android:layout_width="187dp"
        android:layout_height="37dp"
        android:background="#003A2F2F"
        android:fontFamily="casual"
        android:gravity="center"
        android:text="Name"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#7E131111"
        android:textSize="18sp"
        android:typeface="monospace"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.801" />

    <TextView
        android:id="@+id/text_Precent"
        android:layout_width="84dp"
        android:layout_height="69dp"
        android:fontFamily="casual"
        android:gravity="center"
        android:text="%"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#EC1A25"
        android:textSize="50sp"
        android:typeface="monospace"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.629"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.717" />

    <TextView
        android:id="@+id/Precent"
        android:layout_width="87dp"
        android:layout_height="69dp"
        android:fontFamily="casual"
        android:gravity="center"
        android:shadowColor="#F5C21E1E"
        android:text="0"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#EC1A25"
        android:textSize="50sp"
        android:typeface="monospace"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.367"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.717" />

    <TextView
        android:id="@+id/city"
        android:layout_width="187dp"
        android:layout_height="37dp"
        android:background="#003A2F2F"
        android:fontFamily="casual"
        android:gravity="center"
        android:text="City"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#7E131111"
        android:textSize="18sp"
        android:typeface="monospace"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.854" />

    <Button
        android:id="@+id/profile"
        android:layout_width="63dp"
        android:layout_height="23dp"
        android:background="@drawable/shape"
        android:fontFamily="casual"
        android:onClick="myProfile"
        android:text="My Profile"
        android:textAllCaps="false"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.942"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />

    <Button
        android:id="@+id/message"
        android:layout_width="225dp"
        android:layout_height="37dp"
        android:background="@drawable/next"
        android:fontFamily="casual"
        android:onClick="message"
        android:text="Start a conversation                 "
        android:textAllCaps="false"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.907" />

    <Button
        android:id="@+id/home"
        android:layout_width="63dp"
        android:layout_height="23dp"
        android:background="@drawable/shape"
        android:fontFamily="casual"
        android:onClick="Home"
        android:text="Home"
        android:textAllCaps="false"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.048"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.977" />

    <com.facebook.login.widget.ProfilePictureView
        android:id="@+id/matchImage"
        android:layout_width="284dp"
        android:layout_height="275dp"
        android:alwaysDrawnWithCache="false"
        android:background="@drawable/frame"
        android:backgroundTint="#7CFFFFFF"
        android:fadingEdge="vertical"
        android:scrollbarStyle="outsideOverlay"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.41"
        app:srcCompat="@android:drawable/alert_dark_frame"
        tools:visibility="visible">

    </com.facebook.login.widget.ProfilePictureView>/>

</androidx.constraintlayout.widget.ConstraintLayout>